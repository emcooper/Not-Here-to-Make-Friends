
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'Log In', :class =>"btn btn-primary"%>
<% end %>

<br><br>
<%= link_to "New User? Create an Account!", new_user_path %>
