<div class="col-md-12">
  <p class ="text-center">Uncheck eliminated contestants.</p><br>
<%= form_for(:weekly_contestants, url: "/admin/weekly_contestants") do |f|  %>

<%= hidden_field_tag :week_id, @week.next_week.id %>
<div class="row">

  <% week.contestants.each do |contestant| %>
  <div class="col-md-4">
    <p class ="no-padding"><label><%= check_box_tag "contestant_ids[]", contestant.id, checked: true %></label><span class=" action-description"><%= contestant.name %></span></p>
  </div>

  <% end %>
</div>

<var></var>
<div class="text-center">
  <%= f.submit "Submit Roster", :class => "btn btn-primary save-button" %>
</div>
<% end %>
</div>
